<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yuanda.cy_professional_select_stock">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name="com.yuanda.huixuangu.base.HuiXuanGuApplication"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/SlpashTheme">
<!-- App适配尺寸 -->
        <meta-data
            android:name="design_width_in_dp"
            android:value="375"/>
        <meta-data
            android:name="design_height_in_dp"
            android:value="650"/>
        <activity android:name="com.yuanda.huixuangu.ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.yuanda.huixuangu.ui.activity.TestActivity"/>

        <uses-library android:name="org.apache.http.legacy" android:required="false"/>
    </application>

</manifest>